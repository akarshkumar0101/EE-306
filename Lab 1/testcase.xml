<?xml version="1.0"?>
<test-suite>
 <test-case>
  <name>1: Zero Rotate</name>
  <has-max-executions>1</has-max-executions>
  <max-executions>100000000</max-executions>
  <fully-randomize>1</fully-randomize>
   <input>
    <test-register><register>R0</register><value>xABCD</value></test-register>
	<test-register><register>R1</register><value>0</value></test-register>
   </input>
   <output>
    <test-register><register>R0</register><value>xABCD</value></test-register>
   </output>
 </test-case>
 
